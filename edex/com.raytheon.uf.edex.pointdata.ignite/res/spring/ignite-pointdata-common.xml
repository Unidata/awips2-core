<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="ignitePointDataCacheConfigurator" class="com.raytheon.uf.edex.pointdata.ignite.IgnitePointDataCacheConfigurator">
        <constructor-arg ref="pluginRegistry"/>
        <constructor-arg ref="ignitePluginRegistry"/>
        <constructor-arg value="pointDataStore"/>
    </bean>

</beans>