<class 
    name="com.raytheon.edex.db.objects.ihfs.Unitgraph" 
    table="unitgraph"
>

    <composite-id>
        <key-property 
            name="areaId" 
            column="area_id" 
            type="java.lang.String"
            length="8"
        />
        <key-property 
            name="model" 
            column="model" 
            type="java.lang.String"
            length="10"
        />
        <key-property 
            name="dur" 
            column="dur" 
            type="java.lang.Integer"
            length="10"
        />
        <key-property 
            name="ordinal" 
            column="ordinal" 
            type="java.lang.Integer"
            length="10"
        />
        <!-- bi-directional many-to-one association to Riverstat -->
        <key-many-to-one
        	name="riverstat"
	        class="com.raytheon.edex.db.objects.ihfs.Riverstat"
	    >
    	    <column name="lid" />
	    </key-many-to-one>
    </composite-id>    

    <property
        name="discharge"
        type="double"
        column="discharge"
        not-null="true"
        length="17"
    />

    <!-- Associations -->
  

</class>
