<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<bundle>
    <displayList>
        <displays xsi:type="mapRenderableDisplay" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <descriptor xsi:type="mapDescriptor">
                 <resource>
                     <loadProperties>
                         <capabilities>
                             <capability xsi:type="colorableCapability" colorAsString="#ffffff" />
                            <capability xsi:type="labelableCapability" labelField="name" />
                            <capability xsi:type="outlineCapability" lineStyle="SOLID" outlineOn="true" outlineWidth="1" />
                            <capability xsi:type="densityCapability" density="0.4"/>
                        </capabilities>
                        <resourceType>PLAN_VIEW</resourceType>
                    </loadProperties>
                    <properties isSystemResource="false" isBlinking="false" isMapLayer="true" isHoverOn="false"
                        isVisible="true">
                        <pdProps maxDisplayWidth="100000000" minDisplayWidth="0" />
                    </properties>
                    <resourceData xsi:type="dbPointMapResourceData">
                    	<column name="name" expression="city_name" />
                        <table>mapdata.world_cities</table>
                        <mapName>World Cities</mapName>
                        <constraint>cntry_name not in ('Saudi Arabia')</constraint>
                        <constraint>pop_class in ('500,000 to 999,999', '1,000,000 to 4,999,999', '5,000,000 and greater')</constraint>
                    </resourceData>
                </resource>
            </descriptor>
        </displays>
    </displayList>
</bundle>

